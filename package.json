{
	"name": "flowky",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"preview": "vite preview",
		"test": "vitest",
		"test:unit": "vitest --project unit",
		"test:storybook": "vitest --project storybook",
		"test:ui": "vitest --ui",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"generate-scss-types": "typed-scss-modules src --watch",
		"chromatic": "env-cmd -f .env.local npx chromatic --exit-zero-on-changes --project-token=$CHROMATIC_PROJECT_TOKEN"
	},
	"dependencies": {
		"@types/node": "^24.0.4",
		"clsx": "^2.1.1",
		"lucide-react": "^0.522.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-router": "^7.6.3",
		"sass": "^1.89.2",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^4.0.1",
		"@eslint/js": "^9.25.0",
		"@storybook/addon-a11y": "^9.0.13",
		"@storybook/addon-docs": "^9.0.13",
		"@storybook/addon-onboarding": "^9.0.13",
		"@storybook/addon-vitest": "^9.0.13",
		"@storybook/react-vite": "^9.0.13",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.2",
		"@vitejs/plugin-react": "^4.4.1",
		"@vitest/browser": "3.2.4",
		"@vitest/coverage-v8": "3.2.4",
		"chromatic": "^13.0.1",
		"env-cmd": "^10.1.0",
		"eslint": "^9.25.0",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.19",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-sonarjs": "^3.0.3",
		"eslint-plugin-storybook": "^9.0.13",
		"eslint-plugin-unused-imports": "^4.1.4",
		"globals": "^16.0.0",
		"jsdom": "^26.1.0",
		"markdown-to-jsx": "^7.7.8",
		"playwright": "^1.53.1",
		"prettier": "^3.6.1",
		"storybook": "^9.0.13",
		"typed-scss-modules": "^8.1.1",
		"typescript": "~5.8.3",
		"typescript-eslint": "^8.30.1",
		"vite": "^6.3.5",
		"vite-plugin-checker": "^0.9.3",
		"vitest": "^3.2.4"
	}
}
